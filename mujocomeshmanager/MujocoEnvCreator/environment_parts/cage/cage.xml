<mujoco model="cage">
  <compiler convexhull="true" angle="radian" meshdir="environment_parts/" />
  <option integrator="implicitfast" timestep="0.004" jacobian="dense">
    <flag multiccd="enable"/>
  </option>
  <size nkey="1"/>
  <visual>
    <global azimuth="-120" elevation="-20"/>
        <quality shadowsize="2048"/>
        <headlight ambient="0.3 0.3 0.3" diffuse="0.6 0.6 0.6" specular="0.1 0.1 0.1"/>
    </visual>
  <!--statistic meansize="0.721049" extent="1" center="0.2 0 0.2"/-->
   <statistic extent="0.3" center="0.3 0 0" meansize=".03"/>
  <default class="main">
    <general ctrlrange="0 1" biastype="affine" gainprm="2000 0 0 0 0 0 0 0 0 0" biasprm="0 -2000 -200 0 0 0 0 0 0 0"/>
    <default class="iiwa">
      <material shininess="0.25"/>
      <site size="0.001 0.005 0.005" group="4" rgba="1 0 0 1"/>
      <general ctrlrange="0 0"/>
      <default class="joint1">
        <joint range="-2.96706 2.96706"/>
        <general ctrlrange="-2.96706 2.96706"/>
        <default class="joint2">
          <joint range="-2.0944 2.0944"/>
          <general ctrlrange="-2.0944 2.0944"/>
        </default>
      </default>
      <default class="joint3">
        <joint range="-3.05433 3.05433"/>
        <general ctrlrange="-3.05433 3.05433"/>
      </default>
    </default>
    <default class="slider"/>
    <default class="linaxis">
      <material shininess="0.25"/>
    </default>
    <default class="cage_class">
      <material shininess="0.25"/>
      <default class="visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2" material="gray"/>
      </default>
      <default class="collision">
        <geom group="3" contype="1" conaffinity="1"/>
      </default>
    </default>
  </default>





  <asset>


    <material name="metal" rgba="0.58 0.58 0.58 1"/>
    <material name="green" rgba="0.0 1.0 0.0 1"/>
    <material name="silicone" rgba="0.1882 0.1882 0.1882 1"/>
    <material name="black" rgba="0.149 0.149 0.149 1"/>
    <material name="wood" rgba="0.56 0.37 0.29 1"/>
    <material name="gray" class="cage_class" rgba="0.4 0.4 0.4 1"/>
    <material name="MatGnd" specular="0.1" shininess=".3" reflectance="0.1" rgba="0.1 0.1 0.1 0.7"/>


      <!-- Pickup holder and Produkt -->

    <texture type="2d" name="texplane" builtin="checker" mark="cross" rgb1="0.2 0.3 0.4" rgb2="0.1 0.15 0.2" markrgb="0.8 0.8 0.8" width="512" height="512"/>
    <!--texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/-->
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="1 1 1" rgb2="1 1 1" markrgb="1 1 1" width="300" height="300"/>
    <material name="alu" class="linaxis" rgba="0.4 0.4 0.4 1"/>
    <material name="matplane" texture="texplane" texuniform="true" reflectance="0.3"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    <mesh scale="1 1 1" name="linear_axis_back" file="cage/assets_cage/linear_axis_back/linear_axis_back.obj"/>
    <mesh scale="1 1 1" name="linear_axis_front" file="cage/assets_cage/linear_axis_front/linear_axis_front.obj"/>
    <mesh scale="1 1 1" name="robot_plate" file="cage/assets_cage/robot_plate/robot_plate.obj"/>
    <mesh scale="1 1 1" name="linear_axis_right" file="cage/assets_cage/linear_axis_right/linear_axis_right.obj"/>
    <mesh name="cage" file="cage/assets_cage/cage/cage.obj" scale="1 1 1"/>

    <mesh name="linear_axis_left" file="cage/assets_cage/linear_axis_left/linear_axis_left.obj"/>

  </asset>

    <!--include file="environments/UR_gripper/ur_gripper.xml"/-->

    <worldbody>

    <!--geom name="floor" size="0 0 0.05" type="plane" material="groundplane"/-->
    <!--light pos="0 0 1.5" dir="0 0 -1" directional="true"/-->
      <!--body name="cage_ground">
        <inertial pos="0 0 0" mass="15" diaginertia="1 1 1"/>
        <geom name="ground" type="plane" size="0 0 0.1" pos="0 0 0" quat="1 0 0 0" material="MatGnd" group="0" class="collision" contype="1" conaffinity="1"/>
    </body-->

    <body name="cage_body">
        <geom pos="0 0 0" type="mesh" contype="0" conaffinity="0" group="0" rgba="0.4 0.4 0.4 1" name="Cage" mesh="cage" />
        <geom name="cage_col_side_right" group="3" contype="1" conaffinity="1" condim="3" type="box" size="0.1 1.2 1.4" pos="1.4 0 1.425" rgba="0.8 0.6 0.4 1"/>
        <geom name="cage_col_side_left" group="3" contype="1" conaffinity="1" condim="3" type="box" size="0.1 1.2 1.4" pos="-1.45 0 1.425" rgba="0.8 0.6 0.4 1"/>
        <geom name="cage_col_top" group="3" contype="1" conaffinity="1" condim="3" type="box" size="1.4 1.2 0.6" pos="0 0 2.45" rgba="0.8 0.6 0.4 1"/>
    </body>


    <body name="linear_axis_back" childclass="linaxis" pos="0.333 0.5161 1.9632" quat="0.5 -0.5 -0.5 0.5">
        <inertial pos="0 0 0" mass="80" diaginertia="0.133333 666.733 666.733"/>
        <geom class="visual" mesh="linear_axis_back"/>
    </body>

    <body name="linear_axis_front" childclass="linaxis" pos="0.333 -0.444 1.9632" quat="0.5 -0.5 -0.5 0.5">
      <body name="linear_axis_left" childclass="linaxis" pos="-0.4755 0.1262 1.565" quat="0.707107 0 0.707107 0">
          <inertial pos="0 0 0" mass="100" diaginertia="166667 166667 166667"/>
          <joint name="x_left" class="slider" pos="0 0 0" axis="1 0 0" type="slide" range="-0.01 0.89"/>
          <geom class="visual" mesh="linear_axis_left"/>
          <geom name="collision_linear_axis_left" group="3" contype="1" conaffinity="1" condim="3" type="box" size="0.1 0.07 1." pos="0 0 0.95" rgba="0.8 0.6 0.4 1"/>


          <body name="offset_zeroing_left" childclass="linaxis" pos="0 0 1.35">
          <body name="robot_plate_left" childclass="linaxis" pos="0 0.0468 0.128" quat="0 0 1 0">
            <inertial pos="0 0 0" mass="100" diaginertia="166667 166667 166667"/>
            <joint name="y_left" class="slider" pos="0 0 0" axis="0 0 1" type="slide" range="-0.195 1.225"/>
            <geom class="visual" mesh="robot_plate"/>
            <geom name="collision_robot_plate_left" group="3" contype="1" conaffinity="1" condim="3" type="box" size="0.15 0.02 0.15" pos="0 0.03 0" rgba="0.8 0.6 0.4 1"/>

            <body name="base_left" childclass="iiwa" pos="0 0.0447 0" quat="0.5 -0.5 -0.5 -0.5">
              <site name="robot_attach_left" pos="0 0 0" size="0.01" rgba="1 0 0 1" group="0"/>
        </body>
      </body>
      </body>
    </body>



         <geom class="visual" mesh="linear_axis_front"/>
        <body name="linear_axis_right" childclass="linaxis" pos="-0.4755 0.1262 -0.879" quat="0.707107 0 0.707107 0">
            <inertial pos="0 0 0" mass="100" diaginertia="166667 166667 166667"/>
            <joint name="x_right" class="slider" pos="0 0 0" axis="-1 0 0" type="slide" range="-0.01 0.89"/>
            <geom class="visual" mesh="linear_axis_right"/>
            <geom name="collision_linear_axis_right" group="3" contype="1" conaffinity="1" condim="3" type="box" size="0.1 0.07 1." pos="0 0 0.95" rgba="0.8 0.6 0.4 1"/>
            <body name="offset_zeroing_right" childclass="linaxis" pos="0 0 1.35">
            <body name="robot_plate_right" childclass="linaxis" pos="0 0.0468 0.128" quat="0 0 1 0">
                <inertial pos="0 0 0" mass="100" diaginertia="166667 166667 166667"/>
                <joint name="y_right" class="slider" pos="0 0 0" axis="0 0 1" type="slide" range="-0.195 1.225"/>
                <geom class="visual" mesh="robot_plate"/>
                <geom name="collision_robot_plate_right" group="3" contype="1" conaffinity="1" condim="3" type="box" size="0.15 0.02 0.15" pos="0 0.03 0" rgba="0.8 0.6 0.4 1"/>
                <body name="base_right" childclass="iiwa" pos="0 0.0447 0" quat="0.5 -0.5 -0.5 -0.5">
                  <site name="robot_attach_right" pos="0 0 0" size="0.01" rgba="1 0 0 1" group="0"/>
                </body>
                </body>
              </body>
            </body>
          </body>
  </worldbody>
  <actuator>
    <motor ctrllimited="true" class="slider"  ctrlrange="-400.0 400.0" joint="y_left" name="actuatorY_left"/>
    <motor ctrllimited="true" class="slider"  ctrlrange="-400.0 400.0" joint="y_right" name="actuatorY_right"/>
    <motor ctrllimited="true" class="slider"  ctrlrange="-400.0 400.0" joint="x_left" name="actuatorX_left"/>
    <motor ctrllimited="true" class="slider"  ctrlrange="-400.0 400.0" joint="x_right" name="actuatorX_right"/>
    <!--general name="actuatorY_left" class="slider" joint="y_left" ctrlrange="-0.195 1.225" gainprm="5000 0 0 0 0 0 0 0 0 0" biasprm="0 -5000 0 0 0 0 0 0 0 0"/>
    <general name="actuatorY_right" class="slider" joint="y_right" ctrlrange="-0.195 1.225" gainprm="5000 0 0 0 0 0 0 0 0 0" biasprm="0 -5000 0 0 0 0 0 0 0 0"/>
    <general name="actuatorX_left" class="slider" joint="x_left" ctrlrange="-0.01 0.89" gainprm="5000 0 0 0 0 0 0 0 0 0" biasprm="0 -5000 0 0 0 0 0 0 0 0"/>
    <general name="actuatorX_right" class="slider" joint="x_right" ctrlrange="-0.01 0.89" gainprm="5000 0 0 0 0 0 0 0 0 0" biasprm="0 -5000 0 0 0 0 0 0 0 0"/-->
  </actuator>

    <contact>
    <exclude body1="linear_axis_left" body2="cage_body"/>
    <exclude body1="linear_axis_right" body2="cage_body"/>
    <exclude body1="robot_plate_left" body2="cage_body"/>
    <exclude body1="robot_plate_right" body2="cage_body"/>
  </contact>
</mujoco>
